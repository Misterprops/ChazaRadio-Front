{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/api/", "/assets/", "*.json"]
  },

  "mimeTypes": {
    ".json": "application/json"
  },

  "responseOverrides": {
    "404": {
      "rewrite": "/index.html"
    }
  },

  "globalHeaders": {
    "Cache-Control": "no-cache, no-store, must-revalidate",
    "X-Frame-Options": "ALLOWALL",
    "Content-Security-Policy": "frame-ancestors 'self' https://www.youtube.com https://youtube.com https://*.youtube.com;"
  }
}
